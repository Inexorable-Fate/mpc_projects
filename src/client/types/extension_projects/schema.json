{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "anyOf": [
    {
      "$ref": "#/definitions/Project"
    },
    {
      "$ref": "#/definitions/Project_1"
    },
    {
      "$ref": "#/definitions/Project_2"
    }
  ],
  "definitions": {
    "Card": {
      "properties": {
        "back": {
          "$ref": "#/definitions/CardFace"
        },
        "count": {
          "type": "number"
        },
        "front": {
          "$ref": "#/definitions/CardFace"
        }
      },
      "required": [
        "count"
      ],
      "type": "object"
    },
    "CardFace": {
      "properties": {
        "Exp": {
          "type": "string"
        },
        "Height": {
          "type": "number"
        },
        "ID": {
          "type": "string"
        },
        "Name": {
          "type": "string"
        },
        "SourceID": {
          "type": "string"
        },
        "Width": {
          "type": "number"
        }
      },
      "required": [
        "Exp",
        "Height",
        "ID",
        "Name",
        "SourceID",
        "Width"
      ],
      "type": "object"
    },
    "Part": {
      "properties": {
        "cards": {
          "items": {
            "$ref": "#/definitions/Card"
          },
          "type": "array"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "cards",
        "name"
      ],
      "type": "object"
    },
    "Part_1": {
      "properties": {
        "cards": {
          "items": {
            "$ref": "#/definitions/Card"
          },
          "type": "array"
        },
        "code": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "cards",
        "code",
        "name"
      ],
      "type": "object"
    },
    "Project": {
      "properties": {
        "cards": {
          "items": {
            "$ref": "#/definitions/Card"
          },
          "type": "array"
        },
        "code": {
          "type": "string"
        },
        "version": {
          "const": 1,
          "type": "number"
        }
      },
      "required": [
        "cards",
        "code",
        "version"
      ],
      "type": "object"
    },
    "Project_1": {
      "properties": {
        "code": {
          "type": "string"
        },
        "parts": {
          "items": {
            "$ref": "#/definitions/Part"
          },
          "type": "array"
        },
        "version": {
          "const": 2,
          "type": "number"
        }
      },
      "required": [
        "code",
        "parts",
        "version"
      ],
      "type": "object"
    },
    "Project_2": {
      "properties": {
        "parts": {
          "items": {
            "$ref": "#/definitions/Part_1"
          },
          "type": "array"
        },
        "version": {
          "const": 3,
          "type": "number"
        }
      },
      "required": [
        "parts",
        "version"
      ],
      "type": "object"
    }
  }
}