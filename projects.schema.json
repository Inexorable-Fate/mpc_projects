              "Name": {
                "type": "string"
              },
              "ID": {
                "type": "string"
              },
              "SourceID": {
                "type": "string"
              },
              "Exp": {
                "type": "string"
              },
              "Width": {
                "type": "number"
              },
              "Height": {
                "type": "number"
              }
            },
            "required": [
              "Name",
              "ID",
              "SourceID",
              "Exp",
              "Width",
              "Height"
            ]
          },
          "back": {
            "type": "object",
            "properties": {
              "Name": {
                "type": "string"
              },
              "ID": {
                "type": "string"
              },
