{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Projects",
  "type": "object",
  "properties": {
    "projectId": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "image": {
      "type": ["string", "null"]
    },
    "artist": {
      "type": ["string", "null"]
    },
    "info": {
      "type": ["string", "null"]
    },
    "website": {
      "type": ["string", "null"]
    },
    "cardsLink": {
      "type": ["string", "null"]
    },
    "authors": {
