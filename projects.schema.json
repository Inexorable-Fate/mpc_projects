                "type": "string"
              },
              "Exp": {
                "type": "string"
              },
              "Width": {
                "type": "number"
              },
              "Height": {
                "type": "number"
              }
            },
            "required": [
              "Name",
              "ID",
              "SourceID",
              "Exp",
              "Width",
              "Height"
            ]
          }
        },
        "required": [
          "count",
          "front",
          "back"
        ]
      }
    },
    "hash": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "name",
    "description",
    "content",
    "info",
    "website",
    "authors",
    "statuses",
    "tags",
    "lang",
    "created",
    "updated",
    "version",
    "code",
    "cards",
    "hash"
  ]
