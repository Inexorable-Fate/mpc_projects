{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Card": {
      "properties": {
        "back": {
          "$ref": "#/definitions/CardFace"
        },
        "count": {
          "type": "number"
        },
        "front": {
          "$ref": "#/definitions/CardFace"
        }
      },
      "required": [
        "count"
      ],
      "type": "object"
    },
    "CardFace": {
      "properties": {
        "Exp": {
          "type": "string"
        },
        "Height": {
          "type": "number"
        },
        "ID": {
          "type": "string"
        },
        "Name": {
          "type": "string"
        },
        "SourceID": {
          "type": "string"
        },
        "Width": {
          "type": "number"
        }
      },
      "required": [
        "Exp",
        "Height",
        "ID",
        "Name",
        "SourceID",
        "Width"
      ],
      "type": "object"
    },
    "Option": {
      "properties": {
        "name": {
          "type": "string"
        },
        "parts": {
          "items": {
            "$ref": "#/definitions/Part"
          },
          "type": "array"
        }
      },
      "required": [
        "name",
        "parts"
      ],
      "type": "object"
    },
    "Part": {
      "properties": {
        "cards": {
          "items": {
            "$ref": "#/definitions/Card"
          },
          "type": "array"
        },
        "code": {
          "type": "string"
        },
        "enabled": {
          "type": "boolean"
        },
        "name": {
          "type": "string"
        },
        "size": {
          "type": "string"
        }
      },
      "required": [
        "cards",
        "code",
        "name",
        "size"
      ],
      "type": "object"
    }
  },
  "properties": {
    "artist": {
      "type": [
        "null",
        "string"
      ]
    },
    "authors": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "cardsLink": {
      "type": [
        "null",
        "string"
      ]
    },
    "created": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "hash": {
      "type": "string"
    },
    "hidden": {
      "type": "boolean"
    },
    "info": {
      "type": [
        "null",
        "string"
      ]
    },
    "investigatorCount": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "options": {
      "items": {
        "$ref": "#/definitions/Option"
      },
      "type": "array"
    },
    "projectIds": {
      "additionalProperties": {
        "type": "string"
      },
      "type": "object"
    },
    "scenarioCount": {
      "type": "number"
    },
    "statuses": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "tags": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "updated": {
      "type": "string"
    },
    "version": {
      "const": 6,
      "type": "number"
    },
    "website": {
      "type": [
        "null",
        "string"
      ]
    }
  },
  "required": [
    "authors",
    "created",
    "description",
    "hash",
    "investigatorCount",
    "name",
    "options",
    "projectIds",
    "scenarioCount",
    "statuses",
    "tags",
    "updated",
    "version"
  ],
  "type": "object"
}